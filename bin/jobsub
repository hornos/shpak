#!/bin/bash
# mcedit TAB: 4 with spaces

### BEGIN HEADER
. $(dirname ${0})/../lib/h.sh
# load libraries
sp_f_load queue
### END HEADER

function __usage() {
  echo "Usage: ${sp_g_bn} [-j jobinfo]"
}

_chk=true
_ji="start.job"

if test $# -eq 0 ; then
  __usage
  exit 1
elif test $# -eq 1 ; then
  if test "${1}" = "-h" ; then
    __usage
    exit 1
  fi
  _ji="${1}"
else
  while getopts hnj: opt; do
    case "$opt" in
      j) _ji=$OPTARG;;
      n) _chk=false;;
      h) __usage; exit 1;;
    esac
  done
fi

sp_f_ptt "${sp_g_bn}"
sp_f_${sp_g_bn} "${_ji}" ${_chk}
