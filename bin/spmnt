#!/bin/bash

# header
# ------------------------------------------------
. $(dirname ${0})/../lib/h.sh
sp_f_load ssh

# main
# ------------------------------------------------
sp_f_ptt "${sp_g_bn}"

for i in "mid" "key" ; do
  f="sp_p_${i}"
  mf="${!f}/${sp_p_ssh_key}SSHFS"
  if test -r "${mf}" ; then
    echo "mounted ${mf}"
  else
    efsmnt "ssh${i}"
  fi
done
